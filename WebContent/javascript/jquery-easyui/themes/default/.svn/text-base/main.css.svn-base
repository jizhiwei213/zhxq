.bdtable {
  background: -webkit-linear-gradient(top,#ffffff 0,#e1e9f8 100%);
  background: -moz-linear-gradient(top,#ffffff 0,#e1e9f8 100%);
  background: -o-linear-gradient(top,#ffffff 0,#e1e9f8 100%);
  background: linear-gradient(to bottom,#ffffff 0,#e1e9f8 100%);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#ffffff,endColorstr=#e1e9f8,GradientType=0);
  color:#222;}
.bdtable input { border:1px solid #8fb7dc; height:25px;font-size:14px;}
.bdtable input[type="radio"] { border:none;height:25px;font-size:14px;}
.bdtable select { border:1px solid #8fb7dc;height:25px;font-size:14px;}
.bdtable textarea { border:1px solid #8fb7dc;font-size:14px;}
.bdtable td {height:28px;}

