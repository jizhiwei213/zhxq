/*自定义样式 Jiang 20150513*/
.datagrid-cell,
.datagrid-cell-group,
.datagrid-header-rownumber,
.datagrid-cell-rownumber {/*表格字体*/
  font-size: 14px;
}
.datagrid-header,
.datagrid-header-row,
.datagrid-header-inner{/*标题栏*/
  font-size: 14px;
  font-family: 微软雅黑,sans-serif;
  color: #000000;
  height: 29px;
  background-image: -moz-linear-gradient(top, #FFF1C2 0%, #FFDE70 100%); 
  background-image: -webkit-linear-gradient(top, #FFF1C2 0%, #FFDE70 100%); 
  background-image: linear-gradient(top, #FFF1C2 0%, #FFDE70 100%); 
  background-repeat: repeat-x; 
}
.datagrid-header .datagrid-cell span {/*标题字体*/
	font-size: 14px;
}
.datagrid-header-row .datagrid-header-over:hover{/*表格头*/
  background-color: #FFFFFF;  
  background-repeat: repeat-x; 
}
.datagrid-row-selected{/*��ѡ��*/
  background-image: -moz-linear-gradient(top, #E0F0FF 0%, #D2E9FF 100%); 
  background-image: -webkit-linear-gradient(top, #E0F0FF 0%, #D2E9FF 100%); 
  background-image: linear-gradient(top, #E0F0FF 0%, #D2E9FF 100%); 
}
.datagrid-filter-row{/*过滤行*/
  /*
  background-image: -moz-linear-gradient(top, #FFFFFF 0%, #E0E0E0 100%); 
  background-image: -webkit-linear-gradient(top, #FFFFFF 0%, #E0E0E0 100%); 
  background-image: linear-gradient(top, #FFFFFF 0%, #E0E0E0 100%); 
  background-repeat: repeat-x; 
  */
}
.datagrid-editable-input{
  font-size:14px;
  height: 25px;
}
.datagrid-row {/*表格行高*/
  height: 27px;
}
.panel-title {
  font-size: 14px;
}
.panel-body {
  font-size: 14px;
}
.messager-body {
  font-size: 12px;
}
.tree-node,
.tree-title{/*树的字体及高度*/
  font-size: 14px;
  height:20px;
}
.combo .combo-text{/*下拉列表的字体*/
  font-size:14px;
  height:24px;
}
.combobox-item{
  font-size:14px;
  height:20px;
  padding:0;
  padding-top:2px;
}
.bdtable {/*详情表字体*/
  font-size: 14px;
  width:100%;
}
/*自定义样式结束*/

