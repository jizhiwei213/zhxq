.lxr .no { background:#ffffff;display:none; line-height:4px;padding:4px 0 4px 0; background-image:none;color:#000 ;}
.lxr a:link {color:#222;text-decoration: none;}
.lxr a:active {color:#222;text-decoration: none;}
.lxr a:visited {color:#222;text-decoration: none;}
.lxr a:hover {color:#e41c1c;text-decoration: underline;}


