.tdk { width:100%; margin:0px; padding:0px; text-align:center}
.tdk ul{ list-style:none; margin:0; padding:0px}
.tdk li {  border-bottom:1px dotted #98e17e; border-top:1px solid #fff;text-align:left; background: url(images/arrow_left1.png) 12px 10px no-repeat #eef8ea; cursor:pointer;color:#0f7c01 ;padding-left:30px; line-height:27px}
.tdk li:hover {border-bottom:1px dotted #eec093;text-align:left; background: url(images/arrow_left.png) 12px 10px no-repeat #fdf9de; color:#e15a00 ;}
.tdk .no { border-bottom:1px dotted #eec093;padding:4px 0 4px 0; background:#ffffff; text-align:center;display:none; line-height:4px;}
.tdk a:link {color:#0b7b03;text-decoration: none;}
.tdk a:active {color:#0b7b03;text-decoration: none;}
.tdk a:visited {color:#0b7b03;text-decoration: none;}
.tdk a:hover {color:#e25a00;text-decoration: none;}

.lxr { width:100%; margin:0px; padding:0px; border-top:1px solid #acc9e4}
