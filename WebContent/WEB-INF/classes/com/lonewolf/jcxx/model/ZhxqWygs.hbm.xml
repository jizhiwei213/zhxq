<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.lonewolf.jcxx.model.ZhxqWygsModel" table="zhxq_wygs">
        <id name="id" type="java.lang.String">
            <column name="ID" length="32" />
            <generator class="uuid.hex" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="100" />
        </property>
         <property name="gsdz" type="java.lang.String">
            <column name="gsdz" length="300" />
        </property>
         <property name="fuzeren" type="java.lang.String">
            <column name="fuzeren" length="100" />
        </property>
          <property name="fuzerentel" type="java.lang.String">
            <column name="fuzerentel" length="20" />
        </property>
         <property name="remark" type="java.lang.String">
            <column name="remark" length="500" />
        </property>
		<property name="insertTime" />
        <property name="updateTime" />
    </class>
</hibernate-mapping>
